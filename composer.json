{
  "name": "slim/http",
  "type": "library",
  "description": "Doper's PSR-7 implementation",
  "keywords": ["psr7","psr-7","http"],
  "homepage": "http://doper.top",
  "license": "MIT",
  "authors": [
    {
      "name": "jiabin",
      "email": "caojiabin2012@gmail.com",
      "homepage": "http://www.nanjiabin.cn"
    }
  ],
  "require": {
    "php": ">=5.6",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.5",
    "phpunit/phpunit": "^5.7|^6.0",
    "php-http/psr7-integration-tests": "dev-master"
  },
  "provide": {
    "psr/http-message-implementation": "1.0"
  },
  "conflict": {
    "doper/doper": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Doper\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Doper\\Tests\\Http\\": "tests/"
    }
  }
}
